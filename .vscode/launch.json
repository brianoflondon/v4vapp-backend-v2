{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "consoleName": "${file}",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Notification Bot",
            "type": "debugpy",
            "consoleName": "Template App Help",
            "request": "launch",
            "program": "src/notification_bot_setup.py",
            "args": [
                // "run",
                // "@brianoflondon_bot",

                // "setup",
                // "bad token"
                "notify",
                "@brianoflondon_bot",
                "your message goes here [link](https://www.google.com)",
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "Hive Monitor",
            "type": "debugpy",
            "consoleName": "Hive Monitor",
            "request": "launch",
            "program": "${workspaceFolder}/src/hive_monitor_v2.py",
            "args": [
                "v4vapp",
                "brianoflondon",
                "v4vapp.tre",
                "v4vapp.dhf"
            ],
            "justMyCode": true,
        },
        {
            "name": "Umbrel destination lookup",
            "type": "debugpy",
            "consoleName": "Umbrel Destination",
            "request": "launch",
            "program": "${workspaceFolder}/src/dest_alias_fill.py",
            "args": [
                "lnd_monitor_v2_umbrel",
                "umbrel"
            ],
        },
        {
            "name": "Voltage destination lookup",
            "type": "debugpy",
            "consoleName": "Voltage Destination",
            "request": "launch",
            "program": "${workspaceFolder}/src/dest_alias_fill.py",
            "args": [
                "lnd_monitor_v2_voltage",
                "voltage"
            ],
        },
        {
            "name": "CLI Help",
            "type": "debugpy",
            "consoleName": "CLI Help",
            "request": "launch",
            "program": "src/lnd_monitor_v2.py",
            "args": [
                "--help"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "Umbrel-proxy Monitor v2",
            "type": "debugpy",
            "consoleName": "Umbrel-proxy Monitor v2",
            "request": "launch",
            "program": "src/lnd_monitor_v2.py",
            "args": [
                "umbrel-proxy"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "Umbrel Monitor v2",
            "type": "debugpy",
            "consoleName": "Umbrel Monitor v2",
            "request": "launch",
            "program": "src/lnd_monitor_v2.py",
            "args": [
                "umbrel"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "Voltage Monitor v2",
            "type": "debugpy",
            "consoleName": "Voltage Monitor v2",
            "request": "launch",
            "program": "src/lnd_monitor_v2.py",
            "args": [
                "voltage"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "Voltage",
            "type": "debugpy",
            "consoleName": "Voltage",
            "request": "launch",
            "program": "src/lnd_node_monitor.py",
            "args": [
                "voltage"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "Umbrel",
            "type": "debugpy",
            "consoleName": "Umbrel",
            "request": "launch",
            "program": "src/lnd_node_monitor.py",
            "args": [
                "umbrel"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "Umbrel Proxy",
            "type": "debugpy",
            "consoleName": "Umbrel Proxy",
            "request": "launch",
            "program": "src/lnd_node_monitor.py",
            "args": [
                "umbrel-proxy"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "Invoice Experiments",
            "type": "debugpy",
            "consoleName": "Invoice Experiments",
            "request": "launch",
            "program": "src/invoice_experiments.py",
            "console": "integratedTerminal"
        },
        {
            "name": "Log viewer Voltage Docker",
            "type": "debugpy",
            "consoleName": "Voltage Log Viewer",
            "request": "launch",
            "program": "src/stream_logs.py",
            "console": "integratedTerminal",
            "args": [
                "logs_docker/voltage/",
                "-f",
                "--tail",
                "20000"
            ]
        },
        {
            "name": "Log viewer Umbrel Docker",
            "type": "debugpy",
            "consoleName": "Umbrel Log Viewer",
            "request": "launch",
            "program": "src/stream_logs.py",
            "console": "integratedTerminal",
            "args": [
                "logs_docker/umbrel/",
                "-f",
                // "--tail",
                // "200"
            ]
        },
        {
            "name": "Template App Help",
            "type": "debugpy",
            "consoleName": "Template App Help",
            "request": "launch",
            "program": "src/template_app.py",
            "args": [
                "--help"
            ],
            "console": "integratedTerminal"
        },
    ]
}