    "python.testing.pytestArgs": [
        "tests",
        "-rs",
        "--capture=no",
        "--verbose",
        // "--disable-warnings",
        "--log-level=INFO",
        "--log-cli-level=INFO"
    ],
    "python.testing.unittestEnabled": false,
    "python.testing.pytestEnabled": true,
    "python.analysis.extraPaths": [
        "./src/v4vapp_backend_v2/lnd_grpc",
        "./src/v4vapp_backend_v2/config"
    ],
    "flake8.args": [
        "--max-line-length=99"
    ],
    "workbench.colorCustomizations": {
        "activityBar.background": "#4F134B",
        "titleBar.activeBackground": "#6F1B69",
        "titleBar.activeForeground": "#FDF8FD",
        "titleBar.inactiveBackground": "#4F134B",
        "titleBar.inactiveForeground": "#FDF8FD",
        "statusBar.background": "#4F134B",
        "statusBar.foreground": "#FDF8FD",
        "statusBar.debuggingBackground": "#4F134B",
        "statusBar.debuggingForeground": "#FDF8FD",
        "statusBar.noFolderBackground": "#4F134B",
        "statusBar.noFolderForeground": "#FDF8FD"
    },
    "jupyter.notebookFileRoot": "${workspaceFolder}/src/jupyter",
    "[html]": {
        "editor.formatOnSave": true
    },
    "files.associations": {
        "src/v4vapp_backend_v2/admin/templates/**/*.html": "jinja"
    },
    "[jinja]": {
        "editor.formatOnSave": false
    },
    "chat.tools.terminal.autoApprove": {
        "uv": true
    },
    "python.terminal.activateEnvironment": true
}
